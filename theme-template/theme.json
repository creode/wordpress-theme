{
	"$schema": "https://schemas.wp.org/wp/6.5/theme.json",
	"version": 2,
	"patterns": [
	],
	"settings": {
		"useRootPaddingAwareAlignments": true,
		"layout": {
			"contentSize": "1200px",
			"wideSize": "2000px"
		},
		"spacing": {
			"padding": true,
			"units": [
				"rem",
				"px"
			]
		},
		"color": {
			"palette": [
				{
					"color": "#000000",
					"name": "Color A",
